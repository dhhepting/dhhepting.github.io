        <owl:disjointWith rdf:resource="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#ParmesanTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#MushroomTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#MushroomTopping">
