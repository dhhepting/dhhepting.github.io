<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#"
     xml:base="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza">
        <rdfs:comment xml:lang="en">Pizza Ontology from tutorial</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#hasBase -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#hasBase">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#hasIngredient"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#isBaseOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#Pizza"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#PizzaBase"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#hasIngredient -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#hasIngredient">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#isIngredientOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#hasSpiciness -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#hasSpiciness">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#PizzaTopping"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#Spiciness"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#hasTopping -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#hasTopping">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#hasIngredient"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#isToppingOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#Pizza"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#PizzaTopping"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#isBaseOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#isBaseOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#isIngredientOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#isIngredientOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#isIngredientOf"/>
    


    <!-- http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#isToppingOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#isToppingOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#isIngredientOf"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#hasCaloricContent -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#hasCaloricContent">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#Pizza"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#AnchovyTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#AnchovyTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#SeafoodTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#CaperTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#CaperTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#VegetableTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#CheeseTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#CheeseTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#PizzaTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#CheesyPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#CheesyPizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#hasTopping"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#CheeseTopping"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#DeepPanBase -->

    <owl:Class rdf:about="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#DeepPanBase">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#PizzaBase"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#ThinAndCrispyBase"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#GreenPepperTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#GreenPepperTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#PepperTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#HamTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#HamTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#MeatTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#HighCaloriePizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#HighCaloriePizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#hasCaloricContent"/>
                        <owl:someValuesFrom>
                            <rdfs:Datatype>
                                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                                <owl:withRestrictions rdf:parseType="Collection">
                                    <rdf:Description>
                                        <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">400</xsd:minInclusive>
                                    </rdf:Description>
                                </owl:withRestrictions>
                            </rdfs:Datatype>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#JalapenoPepperTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#JalapenoPepperTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#PepperTopping"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#hasSpiciness"/>
                <owl:hasValue rdf:resource="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#Hot"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#MargheritaPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#MargheritaPizza">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#NamedPizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#MozzarellaTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#TomatoTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#hasTopping"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#MozzarellaTopping"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#TomatoTopping"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Pizza with only Tomato and Mozzarella toppings</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#MeatTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#MeatTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#PizzaTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#MediumCaloriePIzza -->

    <owl:Class rdf:about="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#MediumCaloriePIzza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#hasCaloricContent"/>
                        <owl:someValuesFrom>
                            <rdfs:Datatype>
                                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                                <owl:withRestrictions rdf:parseType="Collection">
                                    <rdf:Description>
                                        <xsd:minExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">200</xsd:minExclusive>
                                    </rdf:Description>
                                    <rdf:Description>
                                        <xsd:maxExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">400</xsd:maxExclusive>
                                    </rdf:Description>
                                </owl:withRestrictions>
                            </rdfs:Datatype>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#MozzarellaTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/hepting/ontologies/2022/3/pizza#MozzarellaTopping">
