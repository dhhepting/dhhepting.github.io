	Doi = {10.1109/FBIT.2007.54},
	Month = {October},
	Pages = {409-416},
	Publisher = {IEEE},
	Title = {Evaluation of a Dominance-Based Rough Set Approach to Interface Design},
	W-Type = {conference},
	Year = {2007},
	Bdsk-Url-1 = {https://doi.org/10.1109/FBIT.2007.54}}

